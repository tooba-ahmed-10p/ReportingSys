{
  "env": {
    "jasmine": true,
    //"node": true,
    //"mocha": true,
    "browser": true,
    //"builtin": true
  },
  "globals": {
    "angular":true,
    "_":true,
    "moment":true,
    "Highcharts":true,
    "$":true,
    "Promise": true,
    "saveAs": true,
    "File": true
  },
  "rules": {
    "max-params": ["error", 15],
    "block-scoped-var": 2,
    "comma-style": [
      2,
      "last"
    ],
    "curly": [
      2,
      "all"
    ],
    "dot-notation": [
      2,
      {
        "allowKeywords": true
      }
    ],
    "eqeqeq": [
      2,
      "allow-null"
    ],
    "global-strict": [
      0,
      "never"
    ],
    "guard-for-in": 0,
    "new-cap": 0,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-cond-assign": [
      2,
      "except-parens"
    ],
    "no-debugger": 2,
    "no-console": 2,
    "no-empty": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-parens": 0,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-loop-func": 2,
    "no-multi-str": 2,
    "no-new": 0,
    "no-plusplus": 0,
    "no-proto": 2,
    "no-script-url": 2,
    "no-sequences": 2,
    "no-shadow": 0,
    "no-undef": 2,
    "no-unused-vars": 0,
    "no-with": 2,
    "semi": ["error", "always"],
    "strict": 2,
    "valid-typeof": 2,
    "wrap-iife": [
      0,
      "outside"
    ]/*,
    "no-underscore-dangle": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-implicit-coercion": [
      2,
      {
        "number": true,
        "boolean": true,
        "string": true
      }
    ],
    "no-multiple-empty-lines": 2,
    "one-var": [
      2,
      "never"
    ],
    "operator-linebreak": [
      2,
      "before"
    ],
    "keyword-spacing": [
      2,
      {
        "overrides": {
          "if": {
            "after": false
          },
          "else": {
            "after": false
          },
          "for": {
            "after": false
          },
          "while": {
            "after": false
          },
          "do": {
            "after": false
          },
          "switch": {
            "after": false
          },
          "try": {
            "after": false
          },
          "catch": {
            "after": false
          }
        }
      }
    ],
    "space-before-blocks": [
      2,
      "never"
    ],
    "space-unary-ops": [
      2,
      {
        "words": false,
        "nonwords": false
      }
    ],
    "no-spaced-func": 2,
    "comma-dangle": [
      2,
      "never"
    ],
    "no-trailing-spaces": 2,
    "yoda": [
      2,
      "never"
    ],
    "brace-style": [
      2,
      "stroustrup",
      {
        "allowSingleLine": true
      }
    ],

    "space-infix-ops": 2,
    "newline-after-var": [
      2,
      "always"
    ],
    "camelcase": [
      2,
      {
        "properties": "always"
      }
    ],
    "quotes": [
      2,
      "single",
      "avoid-escape"
    ],
    "max-len": [
      2,
      120
    ]*/
  }
}









